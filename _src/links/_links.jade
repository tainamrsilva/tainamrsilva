section#links.section__block.section__block--darker
  h2.section__title Links
  hr.section__divider
  ul.list
    each links, slug in public.links._data
      li.list__item.list__item--small
        a.list__link(href="#{ links.link }" target="_blank" aria-label="Link #{ links.title }" rel="noopener") 
          span.list__text #{ links.title }
          svg.list__vector(xmlns='http://www.w3.org/2000/svg', width='24', height='24', fill='none', viewbox='0 0 24 24')
            path(fill='#fff', d='M2.53 9.47L0 12l12 12 12-12-2.53-2.53-7.68 7.681V0h-3.58v17.151l-7.68-7.68z')
