{
  "name": "tainamsilva",
  "version": "0.0.1",
  "description": "tainamsilva's website",
  "author": "Matheus Costa <matheusagcosta@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "harp server _src & browser-sync start --no-open --proxy 'localhost:9000' --files '**/*.jade, **/*.scss, **/*.json, **/*.js, **/*.md' --no-notify",
    "build": "harp compile _src docs && touch docs/CNAME && echo tainasilva.com.br >> docs/CNAME",
    "dev:backend": "cd ./backend && npm run develop",
    "prod:backend": "cd ./backend && npm run start",
    "build:backend": "cd ./backend && npm run build"
  },
  "devDependencies": {
    "browser-sync": "^2.27.1",
    "harp": "^0.34.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tainamrsilva/tainamrsilva.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tainamrsilva/tainamrsilva/issues"
  },
  "homepage": "https://github.com/tainamrsilva/tainamrsilva#readme"
}
